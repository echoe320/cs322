void main (){

  int64[] a
  int64 sum
  int64 len

  len <- 10
  a <- new Array(len)
  print(a)
  initArray(a)
  sum <- sumArray(a)
  print(sum)
  return 
}

void initArray (int64[] A){

  A[0] <- 0
  A[1] <- 2
  A[2] <- 0
  A[3] <- 1
  A[4] <- 0
  A[5] <- 2
  A[6] <- 1
  A[7] <- 0
  A[8] <- 0
  A[9] <- 1
  return
}

int64 sumArray (int64[] A) {
  int64 ret
  int64 i
  i <- 0

  :loop
  int64 cmp
  cmp <- i < len
  br cmp :add :end

  :add
  int64 a_val

  a_val <- A[i]
  ret <- ret + a_val
  i <- i + 1
  br :loop

  :end
  return ret
}

